{
  "license": "MIT",
  "dependencies": {
    "esbuild": "^0.15.13",
    "sass": "^1.56.1"
  },
  "devDependencies": {
    "eslint": "^8.27.0",
    "eslint-config-google": "^0.14.0"
  },
  "scripts": {
    "build": "esbuild app/javascript/*.* --bundle --sourcemap --outdir=app/assets/builds --public-path=assets",
    "build:css": "sass ./app/assets/stylesheets/application.sass.scss:./app/assets/builds/application.css --no-source-map --load-path=node_module"
  }
}
